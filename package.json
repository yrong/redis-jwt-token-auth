{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "setenv": "source ../config/.env&&source .env",
    "prebuild": "yarn install&&rm -rf build",
    "build": "webpack",
    "initSchema": "$NEO4J_HOME/bin/neo4j-shell -file ./cypher/initSchema.cyp",
    "start": "node ./server.js",
    "test": "mocha ./test/index.js",
    "sync-mysql": "SYNC_TYPE=mysql node sync.js",
    "sync-nextcloud": "SYNC_TYPE=nextcloud node sync.js",
    "sync-ldap": "SYNC_TYPE=ldap node sync.js",
    "sync-roles": "SYNC_TYPE=acl node sync.js"
  },
  "keywords": [
    "redis-jwt-token",
    "rbac",
    "acl",
    "passport",
    "ldapauth",
    "nextcloud"
  ],
  "author": "Ron.Yang <yrong1997@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "acl-fork": "0.4.10-beta.3",
    "bcryptjs": "^2.3.0",
    "config": "^1.25.1",
    "hashids": "^1.1.0",
    "jsonwebtoken": "^7.2.1",
    "kcors": "^2.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compose": "^3.1.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.3",
    "koa-passport": "^2.2.1",
    "koa-router": "^7.0.1",
    "moment": "^2.13.0",
    "mysql": "^2.12.0",
    "mysql2": "^1.2.0",
    "neo4j-driver": "^1.3.0",
    "ldapauth-fork-plus": "4.0.0-beta.1",
    "lodash": "^4.17.4",
    "parse-neo4j-fork": "0.6.10",
    "passport-ldapauth-fork": "^1.0.1",
    "passport-local": "^1.0.0",
    "uuid": "^3.0.0",
    "webdav": "^0.9.0",
    "scirichon-common": "1.0.1-beta.6",
    "scirichon-response-wrapper": "1.0.0-beta.6"
  },
  "devDependencies": {
    "webpack-builder-advanced": "1.0.0-beta.1"
  }
